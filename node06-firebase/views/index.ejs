<%- include('partials/header', {title: 'Bootstrap Template'} ); %>
  <div class="container-fluid">
  <div class="row">
     <%if ( typeof(user) !== 'undefined'){%> 
      <h4 class="col-sm-12">Hi, <%= user.firstname;%></h4>
      <% } %>
  </div>
  	<% if ( typeof( categories ) !== 'undefined' ){%>
    	<%- include('partials/list-categories', {'categories': categories});%>
    <% } else if ( typeof(category) !== 'undefined' && typeof(items) !== 'undefined' ) {%>
    	<%- include('partials/list-items', {'category': category, 'items': items});%>
    <% }else{ %>
    	<h1>Default Content</h1>
    <%	} %>
  </div>
<%- include('partials/footer'); %>
